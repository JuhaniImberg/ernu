__doc__ = "Functional stuff here!"

map = function (fun, arr)
  res = []
  for val in arr do
    res += fun(val)
  end
  return res
end

reduce = function (fun, arr)
  if len(arr) == 1 do
    return arr[0]
  end
  r = fun(arr[0], arr[1])
  i = 2
  while i < len(arr) do
    v = arr[i]
    r = fun(r, v)
    i += 1
  end
  return r
end