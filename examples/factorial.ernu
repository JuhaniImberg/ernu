factorial = function (n)
  "Calculates the factorial of n recursively"
  if n == 0 do
    1
  else
    n * factorial(n - 1)
  end
end

from dir.definitions1 import coolVariable, coolVariable2, coolVariable3

main = function ()
  for i in range(coolVariable) do
    print(i + ": " + factorial(i))
  end
end

