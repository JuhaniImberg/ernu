factorial = function (n)
  "Calculates the factorial of n recursively"
  if n == 0 do
    1
  else
    n * factorial(n - 1)
  end
end

c = 20

main = function ()
  for i in range(c) do
    print(i, factorial(i))
    if i >= 13 do
      print("starting to overflow? get a better language!")
    end
  end
end

main()