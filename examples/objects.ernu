Cat = class
  init = function (this, name, age)
    this.name = name
    this.age = age
  end
  grow = function (this, val)
    this.age += val
  end
  toString = function (this)
    return "Name: " + this.name + " Age: " + this.age
  end
end

if __executed__ do
  cat = Cat("Matti", 4)
  print(cat.toString())
  cat.grow(4)
  print(cat.toString())
end